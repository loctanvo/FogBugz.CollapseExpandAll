{
  "manifest_version": 2,
  "name": "FogBugz.CollapseAll",
  "version": "0.0.1",

  "description": "Collapse all subcases in FogBugz list view",
    
  "page_action": {
    "default_title": "Button",
    "default_icon": "icon.png"
  },
  
  "content_scripts": [
  {
      "matches": ["https://*.fogbugz.com/*"],
      "js": ["jquery.js", "collapsAll.js"]
  }],
   
  "author": "Loc Tan Vo"
}